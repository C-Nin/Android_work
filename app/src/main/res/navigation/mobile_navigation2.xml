<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_recyclerview">

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.navigationview.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.navigationview.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />
    <fragment
        android:id="@+id/nav_recyclerview"
        android:name="com.example.navigationview.ui.recyclerview.RecyclerViewFragment"
        android:label="漫步者"
        tools:layout="@layout/fragment_recyclerview" />
    <fragment
        android:id="@+id/alldataFragment2"
        android:name="com.example.navigationview.ui.alldata.AlldataFragment"
        android:label="全部品牌"
        tools:layout="@layout/alldata_fragment" />
    <fragment
        android:id="@+id/highendFragment2"
        android:name="com.example.navigationview.ui.highenddata.HighendFragment"
        android:label="高端产品"
        tools:layout="@layout/highend_fragment" />
    <fragment
        android:id="@+id/lowenddataFragment2"
        android:name="com.example.navigationview.ui.lowenddata.LowenddataFragment"
        android:label="特价产品"
        tools:layout="@layout/lowenddata_fragment" />
</navigation>